{
  "name": "json-schema-form",
  "description": "Library for converting JsonSchema to Form in field validation",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "main": "./dist/schema-form.umd.js",
  "module": "./dist/schema-form.es.js",
  "exports": {
    ".": {
      "import": "./dist/schema-form.es.js",
      "require": "./dist/schema-form.umd.js"
    },
    "./dist/styles.css": "./dist/styles.css"
  },
  "files": ["dist"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lilSvensken/json-schema-form.git"
  },
  "dependencies": {
    "@vue/eslint-config-prettier": "^7.1.0",
    "pinia": "^2.1.4",
    "sass": "^1.62.1",
    "vue": "^3.3.2",
    "vue-imask": "^6.6.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "vite": "^4.3.5"
  }
}
